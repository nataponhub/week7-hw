pipeline {
    agent any

    stages {
        
        stage('Welcome to master'){
            steps{
                echo 'Start Program'
            }
        }

        stage('Tigger the Slave Jobs') {
            steps {
                echo 'Start Trigger to send name and age to slave'
                script{
                    def data = readFile(file: 'data.txt')
                    def realData = data.readLines()
                    build job: 'jenkin_slave', parameters: [string(name: 'NAME', value: realData.get(0)), string(name: 'Ages', value: realData.get(1))]
                }
            }
        }
    }
}